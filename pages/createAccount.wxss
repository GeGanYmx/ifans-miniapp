/* pages/createAccount.wxss */
.bk {
  width: 100%;
  height: 1334rpx;
}

.underlying {
  position: relative;
}

.main {
  width: 100%;
  height: 1334rpx;
  /* background: red; */
  position: absolute;
  z-index: 2;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.nickname {
  width: 371rpx;
  height: 77rpx;
  background: rgba(238, 238, 238, 1);
  border: 2rpx solid rgba(0, 0, 0, 1);
  border-radius: 16rpx;
  padding-left: 31rpx;
}

#create {
  width: 663rpx;
  height: 77rpx;
  background: rgba(238, 238, 238, 1);
  border: 2rpx solid rgba(0, 0, 0, 1);
  border-radius: 16rpx;
  margin-top: 551rpx;
  font-size: 34rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: rgba(102, 102, 102, 1);
  display: flex;
  justify-content: center;
  align-items: center;

}

.nickname-pr {
  width: 136rpx;
  height: 27rpx;
  font-size: 28rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 400;
  color: rgba(102, 102, 102, 1);
}

.avatar {
  width: 193rpx;
  height: 193rpx;
  margin-top: 172rpx;
  border-radius: 50%;
  border-style: solid;
  border-width: 1rpx;
  border-color: #999;
}

.tip {
  font-size: 34rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  margin-top: 23rpx;
}