/* pages/createAccount.wxss */
.bk {
    width: 100%;
    height: 1334rpx;
  }
  
  .underlying {
    position: relative;
  }
  
  .main {
    width: 100%;
    position: absolute;
    z-index: 2;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
  }
  
  #create {
    width: 663rpx;
    height: 77rpx;
    background: rgba(238, 238, 238, 1);
    border: 2rpx solid rgba(0, 0, 0, 1);
    border-radius: 16rpx;
    /* margin-top: 551rpx; */
    font-size: 34rpx;
    font-family: PingFang-SC-Bold;
    font-weight: bold;
    color: rgba(102, 102, 102, 1);
    display: flex;
    justify-content: center;
    align-items: center;
    position:fixed;
    bottom:20rpx;
    left:45rpx;

  }
  
  .account-item{
    height:85rpx;display:flex;width:100%;align-items:center;padding-left:80rpx;margin-top:36rpx;
  }

  .avatar{
    height:85rpx;width:85rpx;background:gray;border-radius:50%
  }
  .nickname{
    font-size:34rpx;font-family:PingFang-SC-Medium;font-weight:500;color:rgba(51,51,51,1);margin-left:37rpx;width:440rpx;
  }
  .now{
    font-size:28rpx;font-family:PingFang-SC-Regular;font-weight:400;color:rgba(102,102,102,1);
  }